{
   "name": "typeorm",
   "version": "1.0.0",
   "description": "",
   "main": "src/index.ts",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "dev": "nodemon",
      "build": "tsc",
      "migration:revert": "npm run typeorm migration:revert -- -d ./src/data-source.ts",
      "migration:create": "node --loader ts-node/esm node_modules/typeorm/cli.js migration:create -- -d ./src/data-source.ts",
      "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/migration/users -d ./src/data-source.ts",
      "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "bcrypt": "^6.0.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.2",
      "cors": "^2.8.5",
      "dotenv": "^17.2.3",
      "express": "^5.1.0",
      "jsonwebtoken": "^9.0.2",
      "nodemailer": "^7.0.9",
      "nodemon": "^3.1.10",
      "pg": "^8.14.1",
      "reflect-metadata": "^0.2.2",
      "typeorm": "0.3.27"
   },
   "devDependencies": {
      "@types/node": "^24.6.2",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.8.2"
   }
}
